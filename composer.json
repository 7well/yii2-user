{
    "name": "julatools/yii2-user",
    "description": "Flexible user registration and authentication module for Yii2",
    "keywords": ["yii2", "yii2-user", "user management", "julatools"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/julatools/yii2-user/issues?state=open",
        "forum": "https://gitter.im/julatools/yii2-user",
        "source": "https://github.com/julatools/yii2-user"
    },
    "authors": [
        {
            "name": "Dmitry Erofeev",
            "email": "dmeroff@gmail.com",
            "homepage": "http://dmeroff.ru"
        },
        {
            "name": "Christian Dumhart",
            "email": "christian.dumhart@chd.at",
            "homepage": "http://www.chd.at"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-swiftmailer": "~2.0",
        "yiisoft/yii2-authclient": "~2.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0",
        "codeception/specify": "*",
        "codeception/verify": "*"
    },
    "autoload": {
        "psr-4": {
            "julatools\\user\\": ""
        }
    },
    "extra": {
        "bootstrap": "julatools\\user\\Bootstrap",
        "branch-alias": {
            "dev-master": "0.9.x-dev",
            "dev-develop": "1.0.x-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
